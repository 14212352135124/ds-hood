local Toggle6 = Section9:CreateToggle("Shit talk [J]", nil, function(v)
    local word = {
        'Seed',
        'woah ur horrible',
        'Who taught u to play this game?!?',
        'Im not locking ur just bad lol',
        'uh oh stinky',
        'Oh god may god bless your aim amen'
    }
    local event = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
    
        game:GetService("UserInputService").InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.J then 
        local ran = math.random(1, #word)
           event:FireServer(word[ran], "All")
        end
        end)
end)


